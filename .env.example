# Copy this file to `.env.local` and fill in the real values (do NOT commit `.env.local`)

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_GOOGLE_CLIENT_SECRET=your-google-client-secret
VITE_GOOGLE_REFRESH_TOKEN=your-google-refresh-token
VITE_GOOGLE_TOKEN_URL=https://oauth2.googleapis.com/token

# Spreadsheet IDs
VITE_PAYROLL_SPREADSHEET_ID=your_payroll_spreadsheet_id
VITE_SESSIONS_SPREADSHEET_ID=your_sessions_spreadsheet_id
VITE_EXPIRATIONS_SPREADSHEET_ID=your_expirations_spreadsheet_id
VITE_SALES_SPREADSHEET_ID=your_sales_spreadsheet_id

# AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key

# Environment
VITE_NODE_ENV=development

# OpenAI / AI provider keys
VITE_OPENAI_API_KEY=your_openai_api_key

# Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key


